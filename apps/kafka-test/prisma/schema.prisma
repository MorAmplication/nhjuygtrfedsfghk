datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  createdAt DateTime @default(now())
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  mor       Mor?     @relation(fields: [morId], references: [id])
  morId     String?
  password  String
  roles     Json
  updatedAt DateTime @updatedAt
  username  String   @unique
}

model Mor {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  name      String?
  test String? @(morTest44)
  updatedAt DateTime @updatedAt
  users     User[]
}

model Login {
  accessKey String?
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  testKey   String?
  updatedAt DateTime @updatedAt
}

model Muly {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
}

model Amit {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  nameFirst String?
  updatedAt DateTime @updatedAt
}
